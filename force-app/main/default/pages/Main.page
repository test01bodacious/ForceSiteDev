        evt = evt || window.event;
        var isEscape = false;
        if ("key" in evt) {
            isEscape = (evt.key == "Escape" || evt.key == "Esc");
        } else {
            isEscape = (evt.keyCode == 27);
        }
        if (isEscape) {
            modal.style.display = 'none';
            modalTask.style.display = 'none';
            modalEvent.style.display = 'none';
        }
    };
    </script>
    
